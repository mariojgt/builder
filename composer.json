{
    "name": "mariojgt/builder",
    "description": "Laravel Crud builder with dynamic tables and controller made in vue js",
    "keywords": [
        "laravel",
        "crud-builder",
        "automated-crud"
    ],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "mario",
            "email": "mariojgt2@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mariojgt\\Builder\\": "src"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan route:list"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mariojgt\\Builder\\BuilderProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
